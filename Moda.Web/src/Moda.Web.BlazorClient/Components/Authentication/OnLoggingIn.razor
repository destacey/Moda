
@code {
    protected override async Task OnInitializedAsync()
    {
        var logger = LoggerFactory.CreateLogger<OnLoggingIn>();
        logger.LogInformation("Logging you in...");
        
        Snackbar.Add("Logging you in...", Severity.Info, config =>
        {
            config.BackgroundBlurred = false;
            config.Icon = Icons.Material.Filled.Security;
            config.Onclick = snackbar =>
            {
                return Task.CompletedTask;
            };
        });
    }
}
